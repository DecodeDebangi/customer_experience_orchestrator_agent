{
  "name": "logixal-agent-ai-nodejs-template",
  "version": "1.0.0",
  "description": "Logixal Agent AI - Node.js/TypeScript Template for building intelligent commerce agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "docker:build": "docker build -t logixal-agent:latest .",
    "docker:run": "docker run -d --env-file .env -p 8080:8080 logixal-agent:latest",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js"
  },
  "keywords": [
    "logixal",
    "agent",
    "ai",
    "commerce",
    "vtex",
    "commercetools",
    "a2a",
    "hackathon"
  ],
  "author": "Logixal",
  "license": "PROPRIETARY",
  "dependencies": {
    "@types/pg": "^8.15.6",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.16.3",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "winston": "3.8.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/logixal-genai/agent-ai-nodejs-template.git"
  }
}
